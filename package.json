{
  "name": "appirio-tech-react-components",
  "version": "0.0.0",
  "main": "./index.coffee",
  "devDependencies": {
    "appirio-tech-webpack-config": "0.x.x",
    "eslint": "^2.3.0",
    "eslint-plugin-react": "^4.2.0",
    "babel-core": "^6.6.5",
    "chai": "^3.5.0",
    "cheerio": "^0.20.0",
